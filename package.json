{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --testTimeout -u",
    "test:watch": "node ./node_modules/jest/bin/jest.js  -u --watchAll --coverage --testTimeout",
    "open:coverage": "TZ=UTC CI=true yarn test --coverage -u && open ./coverage/lcov-report/index.html",
    "test:coverage": "TZ=UTC CI=true yarn test --coverage"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/native": "^10.0.27",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-navigation/native": "^5.1.4",
    "@react-navigation/stack": "^5.2.9",
    "@types/jest": "^25.1.4",
    "axios": "^0.19.2",
    "constate": "^2.0.0",
    "expo": "^36.0.2",
    "expo-linear-gradient": "~8.0.0",
    "expo-sharing": "~8.0.0",
    "jest-config": "^25.2.4",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-reanimated": "^1.7.1",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.4.0",
    "react-native-web": "~0.11.7"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/react": "~16.9.0",
    "@types/react-native": "~0.60.23",
    "babel-preset-expo": "~8.0.0",
    "jest-expo": "^37.0.0",
    "react-native-testing-library": "^1.13.0",
    "typescript": "~3.6.3"
  },
  "jest": {
    "preset": "jest-expo",
    "testMatch": [
      "**/*.test.tsx"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ]
  },
  "private": true
}
